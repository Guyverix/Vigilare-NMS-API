{
    "name": "guyverix/nms-api",
    "description": "NMS Project using Slim4 Skeleton to create APIs",
    "keywords": [
        "microframework",
        "rest",
        "router",
        "authentication",
        "psr7"
    ],
    "homepage": "https://gitlab.iwillfearnoevil.com/monitoring/slimframework/nmsApi",
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Hubbard",
            "email": "chubbard@iwillfearnoevil.com",
            "homepage": "http://iwillfearnoevil.com/"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "codes50/validation": "^1.3",
        "firebase/php-jwt": "^5.0",
        "freedsx/snmp": "^0.4.0",
        "gipfl/rrdtool": "^0.2.0",
        "monolog/monolog": "^2.2",
        "php-di/php-di": "^6.2",
        "phpmailer/phpmailer": "^6.8",
        "slim/php-view": "^3.0",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.7",
        "tuupola/slim-jwt-auth": "^3.4",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "jangregor/phpstan-prophecy": "^0.8.1",
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan": "^0.12.70",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S $(hostname -I):8080 -t public",
        "test": "phpunit"
    }
}
